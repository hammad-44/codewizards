{
    "builds": [{
        "src": "codewizards/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfilesbuild" }
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "codewizards/wsgi.py"
        },
        {
            "src":"/static/(.*)",
            "dest": "static/$1"
        }
    ]
    
}
